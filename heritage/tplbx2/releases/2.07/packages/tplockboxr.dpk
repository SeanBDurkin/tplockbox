{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'LockBock 2.0 Runtime'}
{$RUNONLY}
{$IMPLICITBUILD ON}
requires
  visualclx,
  baseclx;

contains
  LbUtils in 'LbUtils.pas',
  LbBigInt in 'LbBigInt.pas',
  LBCIPHER in 'LbCipher.pas',
  LbClass in 'LbClass.pas',
  LbConst in 'LbConst.pas',
  LbDSA in 'LbDSA.pas',
  LBPROC in 'LbProc.pas',
  LbRandom in 'LbRandom.pas',
  LbRSA in 'LbRSA.pas',
  LbString in 'LbString.pas',
  LbAsym in 'LbAsym.pas';
end.
