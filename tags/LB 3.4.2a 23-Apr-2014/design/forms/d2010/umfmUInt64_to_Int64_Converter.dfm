  object memoIntro: TMemo
    Left = 0
    Top = 0
    Width = 562
    Height = 89
    Align = alTop
    Color = clInfoBk
    Lines.Strings = (
      
        'This is a design-support utility intended to assist the extensio' +
        'n of'
      'Delphi programs to include support for Delphi 7.'
      ''
      
        'The Delphi 7 compiler does not recognise literals for UInt64 equ' +
        'al to'
      
        'or greater than the MaxInt for SInt64. This program finds such l' +
        'iterals'
      
        'and converts them into a format that the Delphi 7 compiler can u' +
        'nderstand.'
      ''
      
        'For example, if your source code for a Delphi 2010 looks like th' +
        'is ....'
      ''
      'const X: UInt64 = $8000000000000001;'
      ''
      
        'The Delphi 7 compiler will bork at this. You really want to conv' +
        'ert that'
      'source into this ....'
      ''
      '{$IF compilerversion <= 15}'
      ' // Delphi 7 case.'
      ' // Equally well, we could have used the $ifdef VER150 pragma.'
      'type UInt64 = Int64;'
      ''
      'const X: UInt64 = -$7FFFFFFFFFFFFFFF;'
      ''
      '{$ELSE}'
      'const X: UInt64 = $8000000000000001;'
      '{$ENDIF}'
      ''
      '// if U >= $8000 0000 0000 0000'
      '//     S = -($1 0000 0000 0000 0000 - U)'
      '//   else'
      '//     S = U'
      '')
    ReadOnly = True
    ScrollBars = ssVertical
    TabOrder = 0
  end
